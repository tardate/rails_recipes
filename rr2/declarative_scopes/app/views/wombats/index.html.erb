<h1>Listing wombats</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Bio</th>
    <th>Age</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wombats.each do |wombat| %>
  <tr>
    <td><%= wombat.name %></td>
    <td><%= wombat.bio %></td>
    <td><%= wombat.age %></td>
    <td><%= link_to 'Show', wombat %></td>
    <td><%= link_to 'Edit', edit_wombat_path(wombat) %></td>
    <td><%= link_to 'Destroy', wombat, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wombat', new_wombat_path %>
